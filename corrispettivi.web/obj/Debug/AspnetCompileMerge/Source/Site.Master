                                <telerik:RadMenuItem Text="Corrispettivi" NavigateUrl="~/tabulati/commerciali/corrispettivi/report.aspx" />
                            </Items>
                        </telerik:RadMenuItem>

                        <telerik:RadMenuItem Text="Report" ToolTip="Report" Font-Bold="True" ImageUrl="Image/selectall.png" >
