                        </telerik:RadMenuItem>
                        <telerik:RadMenuItem IsSeparator="true" value="sep7" />
                        <telerik:RadMenuItem Text="Settings" ToolTip="Settings" Font-Bold="True" ImageUrl="Image/settings.png" >
